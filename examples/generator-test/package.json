{
  "name": "@paljs/example-generator-test",
  "version": "1.0.1",
  "private": true,
  "description": "E2E test example for @paljs/generator with Prisma 7",
  "type": "module",
  "scripts": {
    "generate": "prisma generate",
    "build": "tsc",
    "test": "bun test",
    "test:generate": "bun run generate && bun test",
    "clean": "rm -rf generated dist node_modules/.prisma",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@paljs/plugins": "workspace:*",
    "@paljs/nexus": "workspace:*",
    "@prisma/adapter-pg": "^7.3.0",
    "graphql": "^16.8.1",
    "nexus": "^1.3.0"
  },
  "devDependencies": {
    "@paljs/generator": "workspace:*",
    "@types/node": "^22.10.6",
    "prisma": "^7.3.0",
    "typescript": "^5.8.3"
  }
}
